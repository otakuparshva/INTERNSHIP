{
  "version": 2,
  "name": "internship-05",
  "projectId": "prj_OmOBEo5LzRCkTVs8ztl2qoEXrthy",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { 
        "distDir": "build",
        "buildCommand": "CI=false npm run build",
        "outputDirectory": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/static/$1"
    },
    {
      "src": "/(.*).ico",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/$1.ico"
    },
    {
      "src": "/(.*).png",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/$1.png"
    },
    {
      "src": "/(.*).json",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/$1.json"
    },
    {
      "src": "/(.*).txt",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/$1.txt"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://recruitment-app-backend.onrender.com/api",
    "REACT_APP_AWS_REGION": "ap-south-1",
    "REACT_APP_AWS_BUCKET_NAME": "resume.selector.bucket"
  }
} 